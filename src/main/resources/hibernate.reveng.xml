<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="MyShop" match-name="categories"/>
  <table-filter match-catalog="MyShop" match-name="custommers"/>
  <table-filter match-catalog="MyShop" match-name="orderdetails"/>
  <table-filter match-catalog="MyShop" match-name="orders"/>
  <table-filter match-catalog="MyShop" match-name="products"/>
  <table-filter match-catalog="MyShop" match-name="users"/>
</hibernate-reverse-engineering>